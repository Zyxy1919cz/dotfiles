# My aliases for ZShell

# ZShell
alias edzsh="vim ~/.config/.zsh/.zshrc"
alias edalias="vim ~/.config/.zsh/local_aliases.zsh"
alias edzplug="vim ~/.config/.zsh/zplug/packages.zsh"
alias edscripts="vim ~/.config/.zsh/local_scripts.zsh"
alias sc="source ~/.config/.zsh/.zshrc"

# Pacman
alias pacin="sudo pacman -Sv"
